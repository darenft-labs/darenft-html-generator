{
  "name": "dare-html-generator",
  "version": "1.0.0",
  "description": "A Webpack 5 template to generate static html files.",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development IS_SINGLE_BUILD=false webpack serve --config config/webpack.config.ts",
    "build": "NODE_ENV=production IS_SINGLE_BUILD=true webpack build --config config/webpack.config.ts",
    "build-dev": "NODE_ENV=development IS_SINGLE_BUILD=true webpack build --config config/webpack.config.ts",
    "lint-ts": "eslint --cache src/**/*.ts",
    "lint-scss": "stylelint --cache src/**/*.{css,scss}"
  },
  "devDependencies": {
